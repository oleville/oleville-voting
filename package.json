{
  "name": "oleville-voting",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node app.js",
    "db:create": "sudo mysql < script/create_databases.sql",
    "db:migrate:up": "sequelize db:migrate",
    "db:migrate:down": "sequelize db:migrate:undo:all",
    "db:drop": "sudo mysql < script/drop_databases.sql",
    "setup": "sequelize db:migrate",
    "test": "jest",
    "prettier": "prettier --write '{lib,migrations,models,routes,script}/**/*.js' '*.js'",
    "prettier:ci": "prettier --list-different '{lib,migrations,models,routes,script}/**/*.js' '*.js' || ( echo \"\nLooks like the above files aren't pretty.  Run prettier, pretty please.\\n\"; exit 1 )"
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "semi": false
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.9",
    "express": "^4.15.5",
    "moment": "^2.20.1",
    "morgan": "~1.8.1",
    "mysql2": "^1.5.1",
    "node-fetch": "^1.7.2",
    "query-string": "^5.0.0",
    "sequelize": "^4.31.2",
    "sequelize-cli": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "^21.2.1",
    "prettier": "^1.10.2"
  }
}
